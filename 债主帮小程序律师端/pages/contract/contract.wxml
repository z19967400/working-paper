<!--pages/contract/contract.wxml-->
<view class="container">
  <view class="contract">
    <view class="home">
      <view style="padding-top:{{isIPhoneX?'18px':'0px'}};" class="nav">
        <view class="title">
          <van-icon bindtap="goBack" custom-class="left" name="arrow-left" />
          <image mode="widthFix" bindtap="goHome" class="logo"
            src="http://file.debteehelper.com/logo/logo_zzbang_03.png"></image>
        </view>
        <view class="text">
          <text>签署合同</text>
        </view>
      </view>
      <view style="margin-top:{{isIPhoneX?'150px':'135px'}} ;" class="neiron-box">
        <view class="neiron">
          <view class="item-box">
            <view class="item-title">
              <view class="shu"></view>
              <vew>签署状态</vew>
            </view>
            <view class="item-text">
              <text>未签署</text>
            </view>
          </view>
          <view class="item-box">
            <view class="item-title">
              <view class="shu"></view>
              <vew>合同信息</vew>
            </view>
            <view class="item-text">
              <view class="row">
                <text class="row-lable">
                  合同编号
                </text>
                <text class="row-value">
                  CCWTB0000049_1-1
                </text>
              </view>
            </view>
          </view>
        </view>
        <view class="bottom-box">
          <button class="bottom-box-btn" bindtap="submit"> 通知签署</button>
          <button class="bottom-box-btn" bindtap="next"> 查看合同</button>
        </view>
      </view>
    </view>
    <!-- 文本提示节点 -->
    <van-toast id="van-toast" />
  </view>
</view>